<!DOCTYPE html>
<html lang='en'>
<head>
  <title><%= ProcessTracking::Application.config.name %></title>
  <%= stylesheet_link_tag(:flash,:formtastic_static,:formtastic_changes,:application) %>
  <%= javascript_include_tag('jquery.min.js','rails.js','application.js') %>

  <%= csrf_meta_tag %>

</head>
<body class='aa-env-<%= Rails.env %>'>
  <div id="flash-holder">
    <% flash_messages do |flash_type, messages| %>
      <%= content_tag(:div, messages, :class => 'flash', :id => "message_#{  flash_type}") %>
    <% end %>
  </div>
  <%= yield %>

  <footer id="main-footer"><%= render partial: 'shared/footer' %></footer>


  <%= yield :page_javascript %>
</body>
</html>



