<!DOCTYPE html>
<html>
<head>
  <title>ProcessTracking</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag(:defaults) %>
  <%= csrf_meta_tag %>
</head>
<body>

<% flash_messages do |flash_type, messages| %>
  <%= content_tag(:div, messages, :class => 'flash', :id => "message_#{  flash_type}") %>
<% end %>

<%= yield %>
<%= yield :page_javascript %>
</body>
</html>



